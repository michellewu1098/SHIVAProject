<CustomLayout background="System/ThemeSystem/LinearGradientBackground.xml">
	<ImageTextView
		id = "titleTextView"
		layout_width = "wrap_content"
		layout_height = "wrap_content"
		layout_centreX = "0.5"
		layout_centreY = "0.08"
		scaleContent = "true"
		keepContentAspectRatio = "true"
		height_constrained = "true"
		icon_position = "left"
		icon_percentSize = "0.25"
		text = "make bigger or smaller"
		
		src = "System/CommandSymbols/ResizeTitle.xml" />
    <VolView id="MainVolView"
             layout_width="0.34"
             layout_height="0.55"
             height_constrained="false"
             width_constrained="false"
             layout_centreX="0.5"
             layout_centreY="0.55" />
    <ImageTextButton id="ScaleXUp"
                     first_focus="1"
                     next_focus_left="RedoButton"
                     next_focus_right="RotateDown"
                     next_focus_down="ScaleUp"
                     visibility="false"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.06"
                     layout_centreY="0.12087"
                     src="System/CommandSymbols/Xup.xml"
                     onClick="buttonHandler"
                     text="make wider"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleXDown"
                     next_focus_left="RedoButton"
                     next_focus_right="RotateDown"
                     next_focus_down="ScaleUp"
                     visibility="false"
                     layout_width="0.03"
                     layout_height="0.053913"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.41"
                     layout_centreY="0.0452174"
                     src="System/CommandSymbols/Xdown.xml"
                     onClick="buttonHandler"
                     text="make narrower"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleYUp"
                     next_focus_right="RotateDown"
                     next_focus_down="ScaleUp"
                     visibility="false"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.243333"
                     layout_centreY="0.127391"
                     src="System/CommandSymbols/Yup.xml"
                     onClick="buttonHandler"
                     text="make longer"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleYDown"
                     next_focus_right="RotateDown"
                     next_focus_down="ScaleUp"
                     visibility="false"
                     layout_width="0.02"
                     layout_height="0.0434783"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.338333"
                     layout_centreY="0.151739"
                     src="System/CommandSymbols/Ydown.xml"
                     onClick="buttonHandler"
                     text="make shorter"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleZUp"
                     next_focus_right="RotateDown"
                     next_focus_down="ScaleUp"
                     visibility="false"
                     layout_width="0.0583354"
                     layout_height="0.105967"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.288889"
                     layout_centreY="0.297391"
                     src="System/CommandSymbols/Zup.xml"
                     onClick="buttonHandler"
                     text="make taller"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleZDown"
                     next_focus_right="RotateDown"
                     next_focus_down="ScaleUp"
                     visibility="false"
                     layout_width="0.0789105"
                     layout_height="0.130955"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.204347"
                     layout_centreY="0.32025"
                     src="System/CommandSymbols/Zdown.xml"
                     onClick="buttonHandler"
                     text="make smaller"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleUp"
                     next_focus_right="ScaleDown"
                     next_focus_down="ScaleReset"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.0694444"
                     layout_centreY="0.280624"
                     src="System/CommandSymbols/ScaleUp.xml"
                     onClick="buttonHandler"
                     text="scale up"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleDown"
                     next_focus_left="ScaleReset"
                     next_focus_right="RotateDown"
                     next_focus_up="ScaleUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.0732425"
                     layout_centreY="0.754643"
                     src="System/CommandSymbols/ScaleDown.xml"
                     onClick="buttonHandler"
                     text="scale down"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="ScaleReset"
                     next_focus_right="ScaleDown"
                     next_focus_up="ScaleUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.07139"
                     layout_centreY="0.517432"
                     src="System/CommandSymbols/ScaleReset.xml"
                     onClick="buttonHandler"
                     text="reset"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="RotateUp"
                     next_focus_left="RotateDown"
                     next_focus_right="BackButton"
                     next_focus_up="RotateLeft"
                     next_focus_down="RotateReset"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.7"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/RotateArrow_Up.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;up"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateDown"
                     next_focus_left="ScaleDown"
                     next_focus_right="RotateUp"
                     next_focus_up="RotateLeft"
                     next_focus_down="RotateRight"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.3"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/RotateArrow_Down.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;down"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateRight"
                     next_focus_left="ScaleDown"
                     next_focus_right="RotateReset"
                     next_focus_up="RotateLeft"
                     layout_width="0.16"
                     layout_height="0.12"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.5"
                     layout_centreY="0.91"
                     src="System/CommandSymbols/RotateArrow_Right.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate right"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateLeft"
                     next_focus_left="RotateDown"
                     next_focus_right="RotateUp"
                     next_focus_down="RotateRight"
                     layout_width="0.16"
                     layout_height="0.12"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.489818"
                     layout_centreY="0.202299"
                     src="System/CommandSymbols/RotateArrow_Left.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate left"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateReset"
                     next_focus_left="RotateRight"
                     next_focus_right="BackButton"
                     next_focus_up="RotateUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.7"
                     layout_centreY="0.89"
                     src="System/CommandSymbols/ResetView.xml"
                     onClick="buttonHandler"
                     text="reset view"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="EyeGazeRest"
                     visibility="false"
                     layout_width="0.16"
                     layout_height="0.28"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.67"
                     layout_centreY="0.23"
                     src="System/CommandSymbols/Rest.xml"
                     eyeGaze_RestButton="true"
                     text="rest"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SelectAbove"
                     next_focus_left="RotateUp"
                     next_focus_right="RotateDown"
                     next_focus_up="BackButton"
                     next_focus_down="SelectBelow"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.935443"
                     layout_centreY="0.438472"
                     src="System/CommandSymbols/SelectAbove.xml"
                     onClick="buttonHandler"
                     text="select next"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SelectBelow"
                     next_focus_left="RotateUp"
                     next_focus_right="RotateDown"
                     next_focus_up="SelectAbove"
                     next_focus_down="UndoButton"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.937396"
                     layout_centreY="0.65625"
                     src="System/CommandSymbols/SelectBelow.xml"
                     onClick="buttonHandler"
                     text="select previous"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="BackButton"
                     next_focus_left="RotateReset"
                     next_focus_right="RotateDown"
                     next_focus_down="SelectAbove"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.12"
                     src="System/CommandSymbols/Back.xml"
                     onClick="buttonHandler"
                     text="back"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="UndoButton"
                     next_focus_left="RotateUp"
                     next_focus_right="RotateDown"
                     next_focus_up="SelectBelow"
                     next_focus_down="RedoButton"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.693357"
                     layout_centreY="0.110371"
                     src="System/CommandSymbols/Undo.xml"
                     onClick="buttonHandler"
                     text="undo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RedoButton"
                     next_focus_left="RotateUp"
                     next_focus_right="RotateDown"
                     next_focus_up="UndoButton"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.818917"
                     layout_centreY="0.113023"
                     src="System/CommandSymbols/Redo.xml"
                     onClick="buttonHandler"
                     text="redo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
</CustomLayout>
