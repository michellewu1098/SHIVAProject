               		layout_width="0.120833"
               		layout_height="0.195463"
               		padding="10"
               		height_constrained="true"
               		width_constrained="false"
               		layout_centreX="0.286237"
               		layout_centreY="0.284722"
               		src="System/CommandSymbols/IncrementalRot.xml"
               		eyeGaze_enable="true"
               		onClick="buttonHandler"
               		text="stop rotating"
               		icon_percentSize="0.75"
               		text_percentSize="0.25"
               		icon_position="top" />
	<ImageTextButton id="RotateUp"
               		next_focus_left="RotateDown"
               		next_focus_right="UndoButton"
