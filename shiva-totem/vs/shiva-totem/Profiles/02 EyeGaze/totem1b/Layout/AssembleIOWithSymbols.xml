               		layout_height="0.193148"
               		padding="10"
               		height_constrained="true"
               		width_constrained="false"
               		layout_centreX="0.207036"
               		layout_centreY="0.900497"
               		src="System/CommandSymbols/ChangeColour.xml"
               		eyeGaze_enable="true"
               		onClick="buttonHandler"
               		text="change colour"
               		icon_percentSize="0.75"
               		text_percentSize="0.25"
               		icon_position="top" />
