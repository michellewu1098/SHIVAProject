               		scan_prev="RotateRight"
               		layout_width="WRAP_CONTENT"
               		layout_height="0.23"
               		padding="10"
               		height_constrained="true"
               		width_constrained="false"
               		layout_centreX="0.274587"
               		layout_centreY="0.493644"
