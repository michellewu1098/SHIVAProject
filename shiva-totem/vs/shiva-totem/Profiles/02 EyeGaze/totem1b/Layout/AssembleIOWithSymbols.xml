<CustomLayout background="System/ThemeSystem/LinearGradientBackground.xml">
	<ScrollContainer layout_width="0.14"
               		layout_height="1"
               		height_constrained="false"
               		width_constrained="false"
               		layout_centreX="0.0752109"
               		layout_centreY="0.495386"
               		scroll_horizontal="true"
               		scroll_vertical="true">
		<LayoutAdapterView id="PrimitiveList"
                 			first_focus="1"
                 			next_focus_left="Load"
                 			next_focus_right="RotateDown"
                 			scan_next="RotateLeft"
                 			scan_prev="Save"
                 			layout_width="WRAP_CONTENT"
                 			layout_height="WRAP_CONTENT"
                 			height_constrained="false"
                 			width_constrained="false">
			<LinearLayout orientation="vertical" />
		</LayoutAdapterView>
	</ScrollContainer>
	<TextView id="titleTextView"
        		layout_width="WRAP_CONTENT"
        		layout_height="WRAP_CONTENT"
        		height_constrained="false"
        		width_constrained="false"
        		layout_centreX="0.5"
        		layout_centreY="0.05"
        		text="SHIVA"
        		text_alignment="1" />
	<VolView id="MainVolView"
       		layout_width="0.4"
       		layout_height="0.6"
       		height_constrained="false"
       		width_constrained="false"
       		layout_centreX="0.503256"
       		layout_centreY="0.530324" />
	<ImageTextButton id="continuousRotation"
               		next_focus_left="RotateRight"
               		next_focus_right="Save"
               		next_focus_up="RotateUp"
               		scan_next="ChangeColour"
               		visibility="true"
               		layout_width="0.120833"
               		layout_height="0.195463"
               		padding="10"
               		height_constrained="true"
               		width_constrained="false"
               		layout_centreX="0.286237"
               		layout_centreY="0.284722"
               		src="System/CommandSymbols/ContinuousRot.xml"
               		eyeGaze_enable="true"
               		onClick="buttonHandler"
               		text="keep rotating"
               		icon_percentSize="0.75"
               		text_percentSize="0.25"
               		icon_position="top" />
	<ImageTextButton id="incrementalRotation"
               		next_focus_left="RotateRight"
               		next_focus_right="Save"
               		next_focus_up="RotateUp"
               		scan_next="ChangeColour"
