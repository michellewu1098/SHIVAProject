               		icon_percentSize="0.7"
               		text_percentSize="0.3"
               		icon_position="top" />
	<ImageTextButton id="New"
               		next_focus_left="PrimitiveList"
               		next_focus_right="RotateLeft"
               		next_focus_down="RotateDown"
               		layout_width="0.0752604"
               		layout_height="0.109815"
               		padding="10"
               		height_constrained="true"
               		width_constrained="false"
               		layout_centreX="0.217318"
               		layout_centreY="0.0654165"
