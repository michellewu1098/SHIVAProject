               		next_focus_down="RotateReset"
               		scan_next="RotateRight"
               		scan_prev="RotateLeft"
               		layout_width="WRAP_CONTENT"
               		layout_height="0.23"
               		padding="10"
               		height_constrained="true"
               		width_constrained="false"
               		layout_centreX="0.720076"
               		layout_centreY="0.502291"
               		src="System/CommandSymbols/RotateArrow_Up.xml"
               		eyeGaze_enable="true"
               		eyeGaze_Repeat="true"
               		onClick="buttonHandler"
               		text="rotate&#x0A;up"
               		text_size="30"
               		icon_percentSize="0.7"
               		text_percentSize="0.3"
               		icon_position="top" />
