<CustomLayout background="System/ThemeSystem/LinearGradientBackground.xml">
    <ImageTextView id="titleTextView"
                   layout_width="WRAP_CONTENT"
                   layout_height="WRAP_CONTENT"
                   height_constrained="true"
                   width_constrained="false"
                   layout_centreX="0.511068"
                   layout_centreY="0.082315"
                   text="drill a hole"
                   icon_percentSize="0.25"
                   text_percentSize="0.75"
                   icon_position="left"
                   src="System/CommandSymbols/DrillTitle.xml" />
    <VolView id="MainVolView"
             layout_width="0.34"
             layout_height="0.55"
             height_constrained="false"
             width_constrained="false"
             layout_centreX="0.5"
             layout_centreY="0.55" />
    <ImageTextButton id="EyeGazeRest"
                     visibility="false"
                     layout_width="0.0623437"
                     layout_height="0.113333"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.935729"
                     layout_centreY="0.384134"
                     src="System/CommandSymbols/Rest.xml"
                     eyeGaze_RestButton="true"
                     text="rest"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="DrillSizeIncrease"
                     first_focus="1"
                     next_focus_left="BackButton"
                     next_focus_right="CrosshairUp"
                     next_focus_down="DrillSizeReset"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.07"
                     layout_centreY="0.254861"
                     src="System/CommandSymbols/DrillIncrease.xml"
                     onClick="buttonHandler"
                     text="increase size"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="DrillSizeDecrease"
                     next_focus_left="BackButton"
                     next_focus_right="CrosshairDown"
                     next_focus_up="DrillSizeReset"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.0673959"
                     layout_centreY="0.755556"
                     src="System/CommandSymbols/DrillDecrease.xml"
                     onClick="buttonHandler"
                     text="decrease size"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="DrillSizeReset"
                     next_focus_left="BackButton"
                     next_focus_right="CrosshairLeft"
                     next_focus_up="DrillSizeIncrease"
                     next_focus_down="DrillSizeDecrease"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.0680469"
                     layout_centreY="0.503472"
                     src="System/CommandSymbols/DrillReset.xml"
                     onClick="buttonHandler"
                     text="reset drill size"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageButton id="CrosshairLeft"
                 next_focus_left="DrillSizeReset"
                 next_focus_right="RotateDown"
                 next_focus_up="CrosshairUp"
                 next_focus_down="CrosshairDown"
                 layout_width="0.114792"
                 layout_height="0.174537"
                 padding="10"
                 height_constrained="false"
                 width_constrained="false"
                 layout_centreX="0.2"
                 layout_centreY="0.5"
                 src="System/CommandSymbols/MoveDrillLeft.xml"
                 eyeGaze_Repeat="true"
                 onClick="buttonHandler" />
    <ImageButton id="CrosshairRight"
                 next_focus_left="RotateUp"
                 next_focus_right="UndoButton"
                 next_focus_up="RotateLeft"
                 next_focus_down="RotateReset"
                 layout_width="0.12"
                 layout_height="0.2"
                 padding="10"
                 height_constrained="false"
                 width_constrained="false"
                 layout_centreX="0.8"
                 layout_centreY="0.5"
                 src="System/CommandSymbols/MoveDrillRight.xml"
                 eyeGaze_Repeat="true"
                 onClick="buttonHandler" />
    <ImageButton id="CrosshairUp"
                 next_focus_left="DrillSizeIncrease"
                 next_focus_right="RotateLeft"
                 next_focus_down="RotateDown"
                 layout_width="0.12"
                 layout_height="0.2"
                 padding="10"
                 height_constrained="false"
                 width_constrained="false"
                 layout_centreX="0.3"
                 layout_centreY="0.25"
                 src="System/CommandSymbols/MoveDrillUp.xml"
                 eyeGaze_Repeat="true"
                 onClick="buttonHandler" />
    <ImageButton id="CrosshairDown"
                 next_focus_left="DrillSizeDecrease"
                 next_focus_right="RotateRight"
                 next_focus_up="RotateDown"
                 next_focus_down="RotateRight"
                 layout_width="0.12"
                 layout_height="0.2"
                 padding="10"
                 height_constrained="false"
                 width_constrained="false"
                 layout_centreX="0.3"
                 layout_centreY="0.75"
                 src="System/CommandSymbols/MoveDrillDown.xml"
                 eyeGaze_Repeat="true"
                 onClick="buttonHandler" />
    <ImageTextButton id="RotateUp"
                     next_focus_left="RotateDown"
                     next_focus_right="CrosshairRight"
                     next_focus_up="RotateLeft"
                     next_focus_down="RotateReset"
                     scan_prev="RotateLeft"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.7"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/RotateArrow_Up.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;up"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateDown"
                     next_focus_left="CrosshairLeft"
                     next_focus_right="RotateUp"
                     next_focus_up="CrosshairUp"
                     next_focus_down="CrosshairDown"
                     scan_prev="RotateRight"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.3"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/RotateArrow_Down.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;down"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateRight"
                     next_focus_left="CrosshairDown"
                     next_focus_right="RotateReset"
                     next_focus_up="RotateLeft"
                     scan_next="RotateDown"
                     layout_width="0.16"
                     layout_height="0.12"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.5"
                     layout_centreY="0.91"
                     src="System/CommandSymbols/RotateArrow_Right.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate right"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateLeft"
                     next_focus_left="CrosshairUp"
                     next_focus_right="RotateUp"
                     next_focus_down="RotateRight"
                     first_scan="1"
                     scan_next="RotateUp"
                     layout_width="0.16"
                     layout_height="0.12"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.5"
                     layout_centreY="0.2"
                     src="System/CommandSymbols/RotateArrow_Left.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate left"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateReset"
                     next_focus_left="RotateRight"
                     next_focus_right="RedoButton"
                     next_focus_up="RotateUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.7"
                     layout_centreY="0.89"
                     src="System/CommandSymbols/ResetView.xml"
                     onClick="buttonHandler"
                     text="reset view"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="PerformDrill"
                     next_focus_left="CrosshairRight"
                     next_focus_right="DrillSizeIncrease"
                     next_focus_up="BackButton"
                     next_focus_down="UndoButton"
                     layout_width="0.186406"
                     layout_height="0.311111"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.885312"
                     layout_centreY="0.806527"
                     src="System/CommandSymbols/DrillCommand.xml"
                     onClick="buttonHandler"
                     text="drill!"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="BackButton"
                     next_focus_left="CrosshairRight"
                     next_focus_right="DrillSizeReset"
                     next_focus_down="PerformDrill"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.12"
                     src="System/CommandSymbols/Back.xml"
                     onClick="buttonHandler"
                     text="back"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="UndoButton"
                     next_focus_left="CrosshairRight"
                     next_focus_right="DrillSizeIncrease"
                     next_focus_up="PerformDrill"
                     next_focus_down="RedoButton"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.690826"
                     layout_centreY="0.12144"
                     src="System/CommandSymbols/Undo.xml"
                     onClick="buttonHandler"
                     text="undo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RedoButton"
                     next_focus_left="RotateReset"
                     next_focus_right="DrillSizeIncrease"
                     next_focus_up="UndoButton"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.811601"
                     layout_centreY="0.122248"
                     src="System/CommandSymbols/Redo.xml"
                     onClick="buttonHandler"
                     text="redo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
</CustomLayout>
