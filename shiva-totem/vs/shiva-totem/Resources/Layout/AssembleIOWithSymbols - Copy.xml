<CustomLayout background="System/ThemeSystem/LinearGradientBackground.xml">
    <ScrollContainer layout_width="0.14"
                     layout_height="1"
                     height_constrained="false"
                     width_constrained="false"
				 
                     scan_next="DuplicateObj"
                     scan_prev="NudgeActivity"	
					 
                     layout_centreX="0.0752109"
                     layout_centreY="0.495386"
                     scroll_horizontal="true"
                     scroll_vertical="true">
        <LayoutAdapterView id="PrimitiveList"
                           
                           next_focus_left="NudgeActivity"
                           next_focus_right="RotateDown"
                           scan_next="DuplicateObj"
                           scan_prev="NudgeActivity"
					   
                           layout_width="WRAP_CONTENT"
                           layout_height="WRAP_CONTENT"
                           height_constrained="false"
                           width_constrained="false">
            <LinearLayout orientation="vertical" />
        </LayoutAdapterView>
    </ScrollContainer>
    <TextView id="titleTextView"
	                     scan_next="PrimitiveList"
						                      first_scan="1"	
              layout_width="WRAP_CONTENT"
              layout_height="WRAP_CONTENT"
              height_constrained="false"
              width_constrained="false"
              layout_centreX="0.5"
              layout_centreY="0.05"
              text="SHIVA"
              text_alignment="1" />
    <VolView id="MainVolView"
             layout_width="0.4"
             layout_height="0.6"
             height_constrained="false"
             width_constrained="false"
             layout_centreX="0.501954"
             layout_centreY="0.501389" />
    <ImageTextButton id="DuplicateObj"
                     next_focus_left="RotateRight"
                     next_focus_right="Save"
                     next_focus_up="RotateUp"
                     scan_next="ChangeColour"
                     scan_prev="RotateLeft"				 
                     layout_width="0.120833"
                     layout_height="0.195463"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.208203"
                     layout_centreY="0.700463"
                     src="System/CommandSymbols/DuplicateObj.xml"
                     onClick="buttonHandler"
                     text="duplicate object"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />    
	<ImageTextButton id="ChangeColour"
                     next_focus_left="RotateRight"
                     next_focus_right="Save"
                     next_focus_up="RotateUp"
                     scan_next="RotateDown"
                     scan_prev="RotateLeft"					 
                     layout_width="0.120833"
                     layout_height="0.193148"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.207036"
                     layout_centreY="0.900497"
                     src="System/CommandSymbols/ChangeColour.xml"
                     onClick="buttonHandler"
                     text="change colour"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
					 
					 
	<ImageButton
		id="ContinuousRotationToggle"
		layout_width="wrap_content"
		layout_height="0.13"
		layout_centreX="0.7"
		layout_centreY="0.5"
		height_constrained="true"
		scaleContent="true"
		keepContentAspectRatio="true"
		icon_position = "top"
		text="Keep&#10;rotating"
		onClick="buttonHandler"
		src="System/CommandSymbols/RotateArrow_Up.xml"
		eyeGaze_Repeat="false"
    
		scan_prev="RotateLeft"
    
		next_focus_left="RotateDown"
		next_focus_right="NudgeUp"
		next_focus_up="RotateLeft"
		next_focus_down="RotateRight"
	/>					 
    <ImageTextButton id="RotateDown"
                     next_focus_left="PrimitiveList"
                     next_focus_right="RotateUp"
                     next_focus_up="New"
                     next_focus_down="RotateRight"					 
                     scan_next="RotateLeft"
                     scan_prev="RotateRight"			 
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.286306"
                     layout_centreY="0.495959"
                     src="System/CommandSymbols/RotateArrow_Down.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;down"
                     text_size="30"
                     icon_percentSize="0.7"
                     text_percentSize="0.3"
                     icon_position="top" />
    <ImageTextButton id="RotateLeft"
                     next_focus_left="New"
                     next_focus_right="RotateUp"
                     next_focus_down="RotateRight"
                     scan_next="RotateRight"
                     scan_prev="imagebutton"
                     layout_width="0.16"
                     layout_height="0.13"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.495655"
                     layout_centreY="0.163563"
                     src="System/CommandSymbols/RotateArrow_Left.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate left"
                     text_size="30"
                     icon_percentSize="0.7"
                     text_percentSize="0.3"
                     icon_position="top" />					 

    <ImageTextButton id="RotateRight"
                     next_focus_left="RotateDown"
                     next_focus_right="RotateReset"
                     next_focus_up="RotateLeft"
                     scan_next="RotateUp"
                     scan_prev="RotateUp"
                     layout_width="0.16"
                     layout_height="0.13"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.5"
                     layout_centreY="0.92"
                     src="System/CommandSymbols/RotateArrow_Right.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate right"
                     text_size="30"
                     icon_percentSize="0.7"
                     text_percentSize="0.3"
                     icon_position="top" />
	<ImageTextButton id="RotateUp"
                     next_focus_left="RotateDown"
                     next_focus_right="UndoButton"
                     next_focus_up="RotateLeft"
                     next_focus_down="RotateReset"
                     scan_next="RotateReset"
                     scan_prev="RotateLeft"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.720076"
                     layout_centreY="0.502291"
                     src="System/CommandSymbols/RotateArrow_Up.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;up"
                     text_size="30"
                     icon_percentSize="0.7"
                     text_percentSize="0.3"
                     icon_position="top" />
    <ImageTextButton id="RotateReset"
                     next_focus_left="RotateRight"
                     next_focus_right="Save"
                     next_focus_up="RotateUp"
                     scan_next="UndoButton"
                     scan_prev="RotateLeft"					 
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.69675"
                     layout_centreY="0.89008"
                     src="System/CommandSymbols/ResetView.xml"
                     onClick="buttonHandler"
                     text="reset view"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
					 
    <ImageTextButton id="UndoButton"
                     next_focus_left="RotateUp"
                     next_focus_right="RedoButton"
                     next_focus_up="EditMenuActivity"
                     next_focus_down="RedoButton"
                     scan_next="RedoButton"
                     scan_prev="RotateLeft"
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.732518"
                     layout_centreY="0.101154"
                     src="System/CommandSymbols/Undo.xml"
                     onClick="buttonHandler"
                     text="undo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />					 
    <ImageTextButton id="RedoButton"
                     next_focus_left="UndoButton"
                     next_focus_right="SwapAbove"
                     next_focus_up="UndoButton"
                     next_focus_down="NudgeActivity"
                     scan_prev="UndoButton"
					 scan_next="EditMenuActivity"
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.828953"
                     layout_centreY="0.100865"
                     src="System/CommandSymbols/Redo.xml"
                     onClick="buttonHandler"
                     text="redo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
					 
    <ImageTextButton id="New"
                     next_focus_left="PrimitiveList"
                     next_focus_right="RotateLeft"
                     next_focus_down="RotateDown"
                     layout_width="0.1"
                     layout_height="0.172315"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.255729"
                     layout_centreY="0.103611"
                     src="System/CommandSymbols/New.xml"
                     onClick="buttonHandler"
                     text="new"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="EyeGazeRest"
                     visibility="false"
                     layout_width="0.0584375"
                     layout_height="0.115648"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.731849"
                     layout_centreY="0.289028"
                     src="System/CommandSymbols/Rest.xml"
                     eyeGaze_RestButton="true"
                     text="rest"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="EditMenuActivity"
                     next_focus_left="RotateUp"
                     next_focus_right="SelectBelow"
                     next_focus_up="DeleteSelected"
                     next_focus_down="UndoButton"
                     scan_next="DeleteSelected"
                     layout_width="0.116927"
                     layout_height="0.207037"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.823759"
                     layout_centreY="0.285319"
                     src="System/CommandSymbols/Edit.xml"
                     onClick="launchActivityHandler"
                     text="edit"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="DeleteSelected"
                     next_focus_left="RotateUp"
                     next_focus_right="SelectAbove"
                     next_focus_down="EditMenuActivity"
                     scan_next="SelectAbove"
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.832223"
                     layout_centreY="0.506042"
                     src="System/CommandSymbols/Delete.xml"
                     onClick="buttonHandler"
                     text="delete"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SelectAbove"
                     next_focus_left="DeleteSelected"
                     next_focus_right="PrimitiveList"
                     next_focus_down="SelectBelow"
                     scan_prev="SwapAbove"
                     scan_next="SelectBelow"					 
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.939349"
                     layout_centreY="0.1"
                     src="System/CommandSymbols/SelectAbove.xml"
                     onClick="buttonHandler"
                     text="select next"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />

    <ImageTextButton id="SelectBelow"
                     next_focus_left="EditMenuActivity"
                     next_focus_right="PrimitiveList"
                     next_focus_up="SelectAbove"
                     next_focus_down="SwapBelow"
                     scan_next="SwapAbove"
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.938698"
                     layout_centreY="0.261157"
                     src="System/CommandSymbols/SelectBelow.xml"
                     onClick="buttonHandler"
                     text="select previous"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SwapAbove"
                     next_focus_left="RedoButton"
                     next_focus_right="PrimitiveList"
                     next_focus_up="SwapBelow"
                     next_focus_down="PrintActivity"
                     scan_next="SwapBelow"
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.938698"
                     layout_centreY="0.424907"
                     src="System/CommandSymbols/SwapNext.xml"
                     onClick="buttonHandler"
                     text="swap next"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SwapBelow"
                     next_focus_left="UndoButton"
                     next_focus_right="PrimitiveList"
                     next_focus_up="SelectBelow"
                     next_focus_down="SwapAbove"
                     scan_next="NudgeActivity"
                     layout_width="0.1"
                     layout_height="0.17"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.939349"
                     layout_centreY="0.584352"
                     src="System/CommandSymbols/SwapPrevious.xml"
                     onClick="buttonHandler"
                     text="swap previous"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />


    <ImageTextButton id="NudgeActivity"
                     next_focus_left="RotateReset"
                     next_focus_right="PrintActivity"
                     next_focus_up="RedoButton"
                     next_focus_down="Save"
                     scan_next="PrimitiveList"
					 first_scan="1"	
                     layout_width="0.105208"
                     layout_height="0.176944"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.939875"
                     layout_centreY="0.766421"
                     src="System/CommandSymbols/Nudge.xml"
                     onClick="launchActivityHandler"
                     text="move"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
					 
    <TextView id="saveConfirmationText"
              visibility="false"
              layout_width="0.0924479"
              layout_height="0.150463"
              height_constrained="true"
              width_constrained="true"
              layout_centreX="0.826954"
              layout_centreY="0.755695"
              text="saved!"
              text_alignment="1" />
    <TextView id="filenameText"
              layout_width="0.240627"
              layout_height="0.0569445"
              height_constrained="false"
              width_constrained="false"
              layout_centreX="0.877082"
              layout_centreY="0.854537"
              text="None"
              text_size="12"
              text_alignment="1" />
    <TextView id="filenamePath"
              layout_width="0.231512"
              layout_height="0.0615743"
              height_constrained="false"
              width_constrained="false"
              layout_centreX="0.882941"
              layout_centreY="0.982182"
              text="None"
              text_size="12"
              text_alignment="1" />
    <ImageTextButton id="Save"
                     next_focus_left="DuplicateObj"
                     next_focus_right="Load"
                     next_focus_up="NudgeActivity"
                     scan_next="imagebutton"
					 eyeGaze_enable="false"
                     layout_width="0.0791665"
                     layout_height="0.114445"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.807327"
                     layout_centreY="0.917449"
                     src="System/CommandSymbols/Save.xml"
                     onClick="saveHandler"
                     text="save"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="Load"
                     next_focus_left="Save"
                     next_focus_right="PrimitiveList"
                     next_focus_up="PrintActivity"
                     layout_width="0.077118"
                     layout_height="0.117214"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.881891"
                     layout_centreY="0.916258"
                     src="System/CommandSymbols/Load.xml"
                     onClick="loadHandler"
                     text="load"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="PrintActivity"
                     next_focus_left="NudgeActivity"
                     next_focus_right="PrimitiveList"
                     next_focus_up="SwapAbove"
                     next_focus_down="Load"
                     layout_width="0.0817534"
                     layout_height="0.116614"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.958524"
                     layout_centreY="0.917461"
                     src="System/CommandSymbols/Print.xml"
                     onClick="launchActivityHandler"
                     text="print/export"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />					 
</CustomLayout>
