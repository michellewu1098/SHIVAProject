<CustomLayout background="System/ThemeSystem/LinearGradientBackground.xml">
    <ImageTextView id="titleTextView"
                   layout_width="WRAP_CONTENT"
                   layout_height="WRAP_CONTENT"
                   height_constrained="true"
                   width_constrained="false"
                   layout_centreX="0.5"
                   layout_centreY="0.08"
                   text="move objects"
                   icon_percentSize="0.25"
                   text_percentSize="0.75"
                   icon_position="left"
                   src="System/CommandSymbols/NudgeTitle.xml" />
    <VolView id="MainVolView"
             layout_width="0.34"
             layout_height="0.55"
             height_constrained="false"
             width_constrained="false"
             layout_centreX="0.5"
             layout_centreY="0.55" />
    <ImageTextButton id="RotateUp"
                     next_focus_left="RotateDown"
                     next_focus_right="DeleteSelected"
                     next_focus_up="RotateLeft"
                     next_focus_down="RotateReset"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.7"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/RotateArrow_Up.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;up"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateDown"
                     next_focus_left="NudgeDown"
                     next_focus_right="RotateUp"
                     next_focus_up="RotateLeft"
                     next_focus_down="RotateRight"
                     layout_width="WRAP_CONTENT"
                     layout_height="0.23"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.3"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/RotateArrow_Down.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate&#x0A;down"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateRight"
                     next_focus_left="NudgeReset"
                     next_focus_right="RotateReset"
                     next_focus_up="RotateLeft"
                     layout_width="0.16"
                     layout_height="0.12"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.5"
                     layout_centreY="0.91"
                     src="System/CommandSymbols/RotateArrow_Right.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate right"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateLeft"
                     next_focus_left="RotateDown"
                     next_focus_right="RotateUp"
                     next_focus_down="RotateRight"
                     layout_width="0.16"
                     layout_height="0.12"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.5"
                     layout_centreY="0.2"
                     src="System/CommandSymbols/RotateArrow_Left.xml"
                     eyeGaze_Repeat="true"
                     onClick="buttonHandler"
                     text="rotate left"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RotateReset"
                     next_focus_left="RotateRight"
                     next_focus_right="SwapBelow"
                     next_focus_up="RotateUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.7"
                     layout_centreY="0.89"
                     src="System/CommandSymbols/ResetView.xml"
                     onClick="buttonHandler"
                     text="reset view"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="EyeGazeRest"
                     visibility="false"
                     layout_width="0.16"
                     layout_height="0.28"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.67"
                     layout_centreY="0.23"
                     src="System/CommandSymbols/Rest.xml"
                     eyeGaze_RestButton="true"
                     text="rest"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="NudgeYpositive"
                     first_focus="1"
                     next_focus_left="SwapBelow"
                     next_focus_right="NudgeYnegative"
                     next_focus_down="NudgeXnegative"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.06"
                     layout_centreY="0.15"
                     src="System/CommandSymbols/NudgeIn.xml"
                     onClick="buttonHandler"
                     text="move in"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="NudgeYnegative"
                     next_focus_left="NudgeYpositive"
                     next_focus_right="RotateDown"
                     next_focus_down="NudgeXpositive"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.18"
                     layout_centreY="0.15"
                     src="System/CommandSymbols/NudgeOut.xml"
                     onClick="buttonHandler"
                     text="move out"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="NudgeXpositive"
                     next_focus_left="NudgeXnegative"
                     next_focus_right="RotateDown"
                     next_focus_up="NudgeYnegative"
                     next_focus_down="NudgeDown"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.18"
                     layout_centreY="0.35"
                     src="System/CommandSymbols/NudgeRight.xml"
                     onClick="buttonHandler"
                     text="move right"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="NudgeXnegative"
                     next_focus_right="NudgeXpositive"
                     next_focus_up="NudgeYpositive"
                     next_focus_down="NudgeUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.06"
                     layout_centreY="0.35"
                     src="System/CommandSymbols/NudgeLeft.xml"
                     onClick="buttonHandler"
                     text="move left"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="NudgeUp"
                     next_focus_right="NudgeDown"
                     next_focus_up="NudgeXnegative"
                     next_focus_down="NudgeReset"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.119245"
                     layout_centreY="0.563889"
                     src="System/CommandSymbols/NudgeUp.xml"
                     onClick="buttonHandler"
                     text="move up"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="NudgeDown"
                     next_focus_left="NudgeUp"
                     next_focus_right="RotateDown"
                     next_focus_up="NudgeXpositive"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.116854"
                     layout_centreY="0.779223"
                     src="System/CommandSymbols/NudgeDown.xml"
                     onClick="buttonHandler"
                     text="move down"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="NudgeReset"
                     next_focus_right="RotateRight"
                     next_focus_up="NudgeUp"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.249453"
                     layout_centreY="0.89375"
                     src="System/CommandSymbols/ResetNudge.xml"
                     onClick="buttonHandler"
                     text="reset move"
                     text_size="20"
                     icon_percentSize="0.8"
                     text_percentSize="0.2"
                     icon_position="top" />
    <ImageTextButton id="SwapAbove"
                     next_focus_left="RedoButton"
                     next_focus_right="NudgeYpositive"
                     next_focus_up="BackButton"
                     next_focus_down="SelectAbove"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.31"
                     src="System/CommandSymbols/SwapNext.xml"
                     onClick="buttonHandler"
                     text="swap next"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SelectAbove"
                     next_focus_left="DeleteSelected"
                     next_focus_right="NudgeYpositive"
                     next_focus_up="SwapAbove"
                     next_focus_down="SelectBelow"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/SelectAbove.xml"
                     onClick="buttonHandler"
                     text="select next"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="DeleteSelected"
                     next_focus_left="RotateUp"
                     next_focus_right="SelectAbove"
                     next_focus_up="RedoButton"
                     next_focus_down="RotateReset"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.82"
                     layout_centreY="0.5"
                     src="System/CommandSymbols/Delete.xml"
                     onClick="buttonHandler"
                     text="delete"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="BackButton"
                     next_focus_left="UndoButton"
                     next_focus_right="NudgeYpositive"
                     next_focus_down="SwapAbove"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.12"
                     src="System/CommandSymbols/Back.xml"
                     onClick="buttonHandler"
                     text="back"
                     text_size="20"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="UndoButton"
                     next_focus_left="RotateUp"
                     next_focus_right="BackButton"
                     next_focus_down="RedoButton"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.685234"
                     layout_centreY="0.117685"
                     src="System/CommandSymbols/Undo.xml"
                     onClick="buttonHandler"
                     text="undo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="RedoButton"
                     next_focus_left="RotateUp"
                     next_focus_right="SwapAbove"
                     next_focus_up="UndoButton"
                     next_focus_down="DeleteSelected"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.814141"
                     layout_centreY="0.113241"
                     src="System/CommandSymbols/Redo.xml"
                     onClick="buttonHandler"
                     text="redo"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SelectBelow"
                     next_focus_left="RotateUp"
                     next_focus_right="NudgeYpositive"
                     next_focus_up="SelectAbove"
                     next_focus_down="SwapBelow"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.69"
                     src="System/CommandSymbols/SelectBelow.xml"
                     onClick="buttonHandler"
                     text="select previous"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
    <ImageTextButton id="SwapBelow"
                     next_focus_left="RotateReset"
                     next_focus_right="NudgeYpositive"
                     next_focus_up="SelectBelow"
                     layout_width="0.12"
                     layout_height="0.2"
                     padding="10"
                     height_constrained="true"
                     width_constrained="false"
                     layout_centreX="0.94"
                     layout_centreY="0.88"
                     src="System/CommandSymbols/SwapPrevious.xml"
                     onClick="buttonHandler"
                     text="swap previous"
                     icon_percentSize="0.75"
                     text_percentSize="0.25"
                     icon_position="top" />
</CustomLayout>
