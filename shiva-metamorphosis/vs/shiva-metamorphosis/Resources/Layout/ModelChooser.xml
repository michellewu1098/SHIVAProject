<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	layout_width="fill_parent"
	layout_height="fill_parent"
	orientation="vertical">
	
	<LinearLayout
		layout_width="fill_parent"
		layout_height="fill_parent"
		layout_weight="0.4"
		orientation="horizontal">
		<TextButton
			id="BackButton"
			layout_width="fill_parent"
			layout_height="fill_parent"
			onClick="backButtonHandler"
			layout_weight="0.25"
			text="Back"
			/>
		
		<TextView
			id="Title"
			layout_width="fill_parent"
			layout_height="fill_parent"
			layout_weight="0.75"
			text_size="50"
			text="Choose Model"
			/>
		</LinearLayout>
		
		
	<LinearLayout
		layout_width="fill_parent"
		layout_height="fill_parent"
		layout_weight="0.4"
		orientation="horizontal">
		<ImageButton
			id="0"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			first_focus="true"
			first_scan="true"
			scan_prev="BackButton"
			next_focus_up="BackButton"
			padding="10"
			/>
		<ImageButton
			id="1"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_left="0"
			scan_prev="0"
			/>
		<ImageButton
			id="2"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_left="1"
			scan_prev="1"
			/>
		<ImageButton
			id="3"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_left="2"
			scan_prev="2"
			/>
	</LinearLayout>
	
	<LinearLayout
		layout_width="fill_parent"
		layout_height="fill_parent"
		layout_weight="0.4"
		orientation="horizontal">
		<ImageButton
			id="4"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_up="0"
			next_focus_left="3"
			scan_prev="3"
			/>
		<ImageButton
			id="5"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_left="4"
			next_focus_up="1"
			scan_prev="4"
			/>
		<ImageButton
			id="6"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_left="5"
			next_focus_up="2"
			scan_prev="5"
			/>
		<ImageButton
			id="7"
			layout_width="fill_parent"
			layout_height="fill_parent"
			src="selector_normal.png"
			onClick="modelSelectButtonHandler"
			padding="10"
			next_focus_left="6"
			next_focus_right="0"
			next_focus_up="3"
			scan_prev="6"
			scan_next="BackButton"
			/>
	</LinearLayout>
		

</LinearLayout>
